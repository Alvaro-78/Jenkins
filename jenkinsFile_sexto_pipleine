pipeline {
	agent any
	parameters {
		string(name: 'PIPELINE_NAME')
		string(name: 'ID_JOB')
	}
	stages {
		stage('Hola Mundo'){
			steps {
				script {
					sh "echo 'hola mundo'"
				}
			}
		}
		stage('Print params') {
			steps {
				script "echo ${PIPELINE_NAME}"
				script "echo ${ID_JOB}"
			}
		}
	}
}